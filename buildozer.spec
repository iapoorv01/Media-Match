[app]

# (str) Title of your application
title = Duplicate File Finder

# (str) Package name
package.name = duplicatefilefinder

# (str) Package domain
package.domain = org.example

# (str) Version of your application
version = 1.0.0

# (str) Source code directory (default is current directory)
source.dir = .

# (list) Application requirements
# comma separated e.g. requirements = sqlite3,kivy
requirements = kivy,opencv-python,imagehash,pillow

# (str) Application icon
# icon = %(source.dir)/icon.png
